    "@babel/types": "^7.22.10",
    "lodash-es": "^4.17.21",
    "prettier": "^3.0.3"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.7",
    "@types/babel__traverse": "^7.20.1",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
